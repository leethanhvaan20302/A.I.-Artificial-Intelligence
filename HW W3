      "cell_type": "code",
      "source": [
        "from numpy.ma.core import less\n",
        "#\n",
        "import numpy as np\n",
        "import skfuzzy as fuzz\n",
        "from skfuzzy import control as ctrl \n",
        "\n",
        "\n",
        "food=ctrl.Antecedent(np.arange(0,11,1),'Food')\n",
        "service=ctrl.Antecedent(np.arange(0,11,1),'Service')\n",
        "\n",
        "tip=ctrl.Consequent(np.arange(10,31,1),'tip')\n",
        "\n",
        "food['poor']=fuzz.trimf(food.universe,[0,0,5])\n",
        "food['average']=fuzz.trimf(food.universe,[0,5,10])\n",
        "food['good']=fuzz.trimf(food.universe,[5,5,10])\n",
        "\n",
        "service['less']=fuzz.trimf(service.universe,[0,0,5])\n",
        "service['average']=fuzz.trimf(service.universe,[0,5,10])\n",
        "service['much']=fuzz.trimf(service.universe,[5,5,10])\n",
        "\n",
        "#food.view()     #food['average'].view()\n",
        "#service.view()\n",
        "#tip.view()\n",
        "\n",
        "\n",
        "rule1=ctrl.Rule(food['poor'] & service['less'],tip['less'])\n",
        "rule2=ctrl.Rule(food['poor']&service['average'],tip['less'])\n",
        "rule3=ctrl.Rule(food['poor']&service['much'],tip['average'])\n",
        "\n",
        "rule4=ctrl.Rule(food['average']&service['less'],tip['less'])\n",
        "rule5=ctrl.Rule(food['average']&service['average'],tip['average'])\n",
        "rule6=ctrl.Rule(food['average']&service['much'],tip['good'])\n",
        "\n",
        "rule7=ctrl.Rule(food['good']&service['less'],tip['average'])\n",
        "rule8=ctrl.Rule(food['good']&service['average'],tip['good'])\n",
        "rule9=ctrl.Rule(food['good']&service['much'],tip['good'])\n",
        "\n",
        "#rule1.view()\n",
        "\n",
        "tipping_ctrl=ctrl.ControlSystem([rule1,rule2,rule3,rule4,rule5,rule6,rule7,rule8,rule9])   # liệt kê ra tất cả các luật\n",
        "tipping=ctrl.ControlSystemSimulation(tipping_ctrl)\n",
        "\n",
        "tipping.input['food']=6.5\n",
        "tipping.input['service']=9.8\n",
        "\n",
        "tipping.compute()\n",
        "print(tipping.output['tip'])\n",
        "tip.view(sim=tipping)\n"
      ],
      "metadata": {
